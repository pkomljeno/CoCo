<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/overlay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragmentshower">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:letterSpacing="0.1"
        android:text="DOBRODOÅ LI!"
        android:textColor="@color/text"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.144" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/back"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:src="@drawable/back"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.444" />


    <EditText
        android:id="@+id/editime12pl"
        android:layout_width="270dp"
        android:layout_height="39dp"
        android:layout_marginTop="300dp"
        android:background="@color/editbckgr"
        android:ems="10"
        android:gravity="center"
        android:hint="Unesite ime"
        android:inputType="textPersonName"
        android:text=""
        android:textColorHint="@color/text"
        app:layout_constraintEnd_toStartOf="@+id/editime22pl"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editime22pl"
        android:layout_width="270dp"
        android:layout_height="39dp"
        android:layout_marginTop="300dp"
        android:background="@color/editbckgr"
        android:ems="10"
        android:gravity="center"
        android:hint="Unesite ime"
        android:inputType="textPersonName"
        android:text=""
        android:textColorHint="@color/text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/editime12pl"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editime12pl2"
        android:layout_width="270dp"
        android:layout_height="39dp"
        android:layout_marginTop="72dp"
        android:background="@color/editbckgr"
        android:ems="10"
        android:gravity="center"
        android:hint="Unesite ime"
        android:inputType="textPersonName"
        android:text=""
        android:textColorHint="@color/text"
        app:layout_constraintEnd_toStartOf="@+id/editime22pl2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editime12pl" />


    <EditText
        android:id="@+id/editime22pl2"
        android:layout_width="270dp"
        android:layout_height="39dp"
        android:layout_marginTop="72dp"
        android:background="@color/editbckgr"
        android:ems="10"
        android:gravity="center"
        android:hint="Unesite ime"
        android:inputType="textPersonName"
        android:text=""
        android:textColorHint="@color/text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/editime12pl2"
        app:layout_constraintTop_toBottomOf="@+id/editime22pl" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/potvrdi2"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:src="@drawable/big_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.625" />


    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Unesite svoja imena s obzirom na raspored sjedenja:"
        android:textColor="@color/text"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/potvrdi2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>